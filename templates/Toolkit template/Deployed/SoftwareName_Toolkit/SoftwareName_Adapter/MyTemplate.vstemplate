      </Folder>
      <ProjectItem ReplaceParameters="true" TargetFileName="SoftwareName_Adapter.cs">SoftwareName_Adapter.cs</ProjectItem>
      <Folder Name="Types" TargetFolderName="Types">
        <ProjectItem ReplaceParameters="true" TargetFileName="Comparer.cs">Comparer.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="DependencyTypes.cs">DependencyTypes.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="NextId.cs">NextId.cs</ProjectItem>
      </Folder>
      <Folder Name="Update" TargetFolderName="Update">
        <ProjectItem ReplaceParameters="true" TargetFileName="_UpdateObjects.cs">_UpdateObjects.cs</ProjectItem>
      </Folder>
    </Project>
  </TemplateContent>
</VSTemplate>