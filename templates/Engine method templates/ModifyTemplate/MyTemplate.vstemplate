<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Item">
  <TemplateData>
    <DefaultName>ModifyMethod.cs</DefaultName>
    <Name>Modify - BHoM Toolkit Engine method</Name>
    <Description>Adds a Modify method to your BHoM Toolkit Engine project.

Modify methods must modify and return the same object type that is passed as the first argument.
		</Description>
    <ProjectType>CSharp</ProjectType>
    <SortOrder>10</SortOrder>
    <Icon>Modify.ico</Icon>
  </TemplateData>
  <TemplateContent>
    <References>
      <Reference>
        <Assembly>BHoM</Assembly>
      </Reference>
    </References>
    <ProjectItem SubType="" TargetFileName="$fileinputname$.cs" ReplaceParameters="true">ModifyTemplate.cs</ProjectItem>
  </TemplateContent>
</VSTemplate>