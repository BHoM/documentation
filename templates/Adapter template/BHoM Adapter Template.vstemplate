<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="ProjectGroup">
    xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">  
    <TemplateData>  
        <Name>BHoM Adapter Template</Name>  
        <Description>Template for creating an adapter to a software</Description>  
		<ProjectType>CSharp</ProjectType>
		<CreateNewFolder>false</CreateNewFolder>
		<Icon>BHoMLogo.ico</Icon>		
    </TemplateData>  
    <TemplateContent>  
        <ProjectCollection>  
            <ProjectTemplateLink ProjectName="$safeprojectname$_Adapter" CopyParameters="true">  
                SoftwareName_Adapter\SoftwareName_Adapter.vstemplate  
            </ProjectTemplateLink>  
            <ProjectTemplateLink ProjectName="$safeprojectname$_Engine" CopyParameters="true">  
                SoftwareName_Engine\SoftwareName_Engine.vstemplate  
            </ProjectTemplateLink>
            <ProjectTemplateLink ProjectName="$safeprojectname$_oM" CopyParameters="true">  
                SoftwareName_oM\SoftwareName_oM.vstemplate  
            </ProjectTemplateLink>  			
        </ProjectCollection>  
    </TemplateContent>  
</VSTemplate>